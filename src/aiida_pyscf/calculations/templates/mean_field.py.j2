from pyscf import scf

mean_field = scf.{{ mean_field.method | default('UKS') }}(structure)
{% if mean_field %}{% for key, value in mean_field.items() %}mean_field.{{ key }} = {{ value|tojson }}
{% endfor %}{% endif %}

